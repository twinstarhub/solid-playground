{
  "name": "solid-repl",
  "description": "Quickly discover what the solid compiler will generate from your JSX template",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid-playground.git"
  },
  "files": [
    "lib"
  ],
  "version": "0.16.0",
  "homepage": "https://playground.solidjs.com",
  "author": "Alexandre Mouton-Brady",
  "module": "lib/index.js",
  "types": "lib/types.d.ts",
  "scripts": {
    "prepare": "patch-package",
    "dev": "vite",
    "build": "vite build && workbox generateSW workbox-config.js",
    "build:lib": "vite build -c vite-lib.config.ts",
    "format": "prettier -w \"{src,playground}/**/*.{js,json,ts,tsx,html,css}\" \"*.{js,ts}\"",
    "prepublishOnly": "pnpm build:lib"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/types": "^7.15.6",
    "@types/babel__standalone": "^7.1.3",
    "@types/dedent": "^0.7.0",
    "@types/workbox-sw": "^4.3.2",
    "assert": "^2.0.0",
    "autoprefixer": "^10.3.6",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.8",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "2.3.8",
    "vite-plugin-solid": "^2.0.3",
    "workbox-cli": "^6.3.0"
  },
  "dependencies": {
    "@amoutonbrady/lz-string": "^0.0.1",
    "@amoutonbrady/solid-heroicons": "^0.9.0-rc.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/standalone": "^7.15.7",
    "@tailwindcss/forms": "^0.3.4",
    "babel-preset-solid": "1.1.5",
    "dedent": "^0.7.0",
    "mitt": "^3.0.0",
    "monaco-editor": "^0.28.1",
    "monaco-editor-textmate": "^3.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "prettier": "^2.4.1",
    "register-service-worker": "^1.7.2",
    "rollup": "^2.58.0",
    "solid-js": "1.1.5"
  }
}
