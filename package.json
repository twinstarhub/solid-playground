{
  "name": "solid-playground",
  "description": "Quickly discover what the solid compiler will generate from your JSX template",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/solid-playground.git"
  },
  "homepage": "http://playground.solid.dev",
  "author": "Alexandre Mouton-Brady",
  "scripts": {
    "dev": "vite",
    "build": "vite build && workbox generateSW workbox-config.js",
    "build-lib": "vite build -c vite-lib.config.ts && tsc --project tsconfig-lib.json",
    "format": "prettier -w \"{src,playground}/**/*.{js,json,ts,tsx,html,css}\"",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/types": "^7.14.2",
    "@types/babel__standalone": "^7.1.3",
    "@types/workbox-sw": "^4.3.1",
    "assert": "^2.0.0",
    "autoprefixer": "^10.2.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.2.15",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "vite": "2.3.2",
    "vite-plugin-solid": "^1.8.0",
    "workbox-cli": "^6.1.5"
  },
  "dependencies": {
    "@amoutonbrady/lz-string": "^0.0.1",
    "@amoutonbrady/solid-heroicons": "^0.7.0",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/standalone": "^7.14.2",
    "@tailwindcss/forms": "^0.3.2",
    "babel-plugin-jsx-dom-expressions": "^0.27.4",
    "babel-preset-solid": "0.26.5",
    "mitt": "^2.1.0",
    "monaco-editor": "^0.24.0",
    "monaco-editor-textmate": "^2.2.2",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "prettier": "^2.3.0",
    "register-service-worker": "^1.7.2",
    "rollup": "^2.47.0",
    "solid-js": "0.26.5",
    "solid-utils": "^0.7.0"
  }
}
