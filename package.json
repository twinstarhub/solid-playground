    "@tailwindcss/forms": "^0.2.1",
    "@types/babel__standalone": "^7.1.3",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^14.14.16",
    "@types/workbox-sw": "^4.3.1",
    "autoprefixer": "^10.1.0",
    "parcel": "^2.0.0-nightly.491",
    "postcss": "^8.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "workbox-cli": "^6.0.2"
  },
  "dependencies": {
    "@amoutonbrady/solid-heroicons": "^0.5.1",
