    "@types/workbox-sw": "4.3.1",
    "autoprefixer": "10.2.4",
    "del-cli": "3.0.1",
    "parcel": "2.0.0-nightly.574",
    "postcss": "8.2.5",
    "sass": "1.32.6",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3",
    "workbox-cli": "6.1.0"
  },
  "dependencies": {
    "@amoutonbrady/lz-string": "^0.0.1",
