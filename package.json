    "autoprefixer": "10.2.3",
    "del-cli": "3.0.1",
    "parcel": "2.0.0-nightly.548",
    "postcss": "8.2.4",
    "sass": "1.32.5",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3",
